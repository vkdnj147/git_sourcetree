<AnchorPane xmlns:fx="http://javafx.com/fxml/1"
	prefHeight="500" prefWidth="1000"
	fx:controller="controller.MainViewController">
	<center>
		<TabPane tabClosingPolicy="UNAVAILABLE" fx:id="mainPane">
			<tabs>
				<Tab text="예약 관리 현황" fx:id="MainView">
					<content>
						<fx:include fx:id="MainViewTab" source="MainViewTab.fxml" />
					</content>
				</Tab>
				<Tab text="전체 이용자 관리" fx:id="TotalCustomer">
					<content>
						<fx:include fx:id="TotalCustomerTab" source="TotalCustomerTab.fxml" />
					</content>
				</Tab>
				<Tab text="직원 관리" fx:id="TotalEmployee">
					<content>
						<fx:include fx:id="TotalEmployeeTab" source="TotalEmployeeTab.fxml" />
					</content>
				</Tab>
				<Tab text="매출 현황" fx:id="TotalSale">
					<content>
						<fx:include fx:id="TotalSaleTab"
							source="TotalSaleTab.fxml" />
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</center>
</AnchorPane>

